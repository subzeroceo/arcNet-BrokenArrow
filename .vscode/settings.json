{
  "C_Cpp_Runner.cCompilerPath": "gcc",
  "C_Cpp_Runner.cppCompilerPath": "g++",
  "C_Cpp_Runner.debuggerPath": "gdb",
  "C_Cpp_Runner.cStandard": "",
  "C_Cpp_Runner.cppStandard": "",
  "C_Cpp_Runner.msvcBatchPath": "",
  "C_Cpp_Runner.useMsvc": false,
  "C_Cpp_Runner.warnings": [
    "-Wall",
    "-Wextra",
    "-Wpedantic",
    "-Wshadow",
    "-Wformat=2",
    "-Wconversion",
    "-Wnull-dereference",
    "-Wsign-conversion"
  ],
  "C_Cpp_Runner.enableWarnings": true,
  "C_Cpp_Runner.warningsAsError": false,
  "C_Cpp_Runner.compilerArgs": [],
  "C_Cpp_Runner.linkerArgs": [],
  "C_Cpp_Runner.includePaths": [],
  "C_Cpp_Runner.includeSearch": [
    "*",
    "**/*"
  ],
  "C_Cpp_Runner.excludeSearch": [
    "**/build",
    "**/build/**",
    "**/.*",
    "**/.*/**",
    "**/.vscode",
    "**/.vscode/**"
  ],
  "C_Cpp_Runner.useAddressSanitizer": false,
  "editor.insertSpaces": false,
  "editor.linkedEditing": true,
  "editor.tabCompletion": "on",
  "editor.stickyTabStops": true,
  "editor.find.autoFindInSelection": "always",
  "editor.formatOnPaste": false,
  "diffEditor.codeLens": true,
  "editor.suggest.shareSuggestSelections": true,
  "files.simpleDialog.enable": true,
  "files.trimTrailingWhitespace": true,
  "workbench.commandPalette.experimental.suggestCommands": true,
  "workbench.commandPalette.experimental.useSemanticSimilarity": true,
  "workbench.editor.mouseBackForwardToNavigate": false,
  "workbench.settings.enableNaturalLanguageSearch": true,
  "problems.sortOrder": "position",
  "html.format.enable": false,
  "less.format.enable": false,
  "scss.format.braceStyle": "collapse",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.guides.bracketPairs": "active",
  "html.format.preserveNewLines": false,
  "editor.defaultFormatter": "AMiner.codegeex",
  "editor.defaultColorDecorators": true,
"editor.detectIndentation": false,
"editor.useTabStops": false
}